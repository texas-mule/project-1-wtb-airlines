{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "passenger_capacity",
    "range",
    "size",
    "speed",
    "useful_load"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        10
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Caravan"
      ],
      "pattern": "^(.*)$"
    },
    "passenger_capacity": {
      "$id": "#/properties/passenger_capacity",
      "type": "integer",
      "title": "The Passenger_capacity Schema",
      "default": 0,
      "examples": [
        14
      ]
    },
    "range": {
      "$id": "#/properties/range",
      "type": "integer",
      "title": "The Range Schema",
      "default": 0,
      "examples": [
        1070
      ]
    },
    "size": {
      "$id": "#/properties/size",
      "type": "number",
      "title": "The Size Schema",
      "default": 0.0,
      "examples": [
        823.872006
      ]
    },
    "speed": {
      "$id": "#/properties/speed",
      "type": "integer",
      "title": "The Speed Schema",
      "default": 0,
      "examples": [
        186
      ]
    },
    "useful_load": {
      "$id": "#/properties/useful_load",
      "type": "integer",
      "title": "The Useful_load Schema",
      "default": 0,
      "examples": [
        3305
      ]
    }
  }
}
